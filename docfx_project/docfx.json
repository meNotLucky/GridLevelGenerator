{
  "metadata": [
      {
          "src": [
              {
                  "src": "..",
                  "files": [
                    "Assets/LevelGenerator/Generator/*.cs",
                    "Assets/LevelGenerator/Utility/GridLevelUtility.cs"
                  ]
              }
          ],
          "globalNamespaceId": "Global",
          "filter": "filterConfig.yml",
          "dest": "api"
      }
  ],
  "build": {
      "globalMetadata": {
          "_appTitle": "Grid Level Generator Documentation",
          "_appFooter": "Grid Level Generator Documentation",
          "_enableSearch": true
      },
      "content": [
          {
              "files": [
                  "toc.yml",
                  "index.md"
              ]
          },
          {
              "src": "api",
              "files": [
                  "*.yml"
              ],
              "dest": "api"
          },
          {
              "src": "articles",
              "files": [
                  "toc.yml",
                  "*.md"
              ],
              "dest": "articles"
          }
      ],
      "overwrite": [
          {
              "src": "..",
              "files": [
                  "Assets/GridLevelGenerator/**/*.md"
              ]
          }
      ],
      "resource": [
          {
              "files": [
                  "resources/**/*"
              ]
          }
      ],
      "sitemap": {
          "baseUrl": "https://menotlucky.github.io/GridLevelGenerator",
          "changefreq": "weekly",
          "fileOptions": {
              "api/*": {
                  "changefreq": "daily"
              }
          }
      },
      "dest": "../docs",
      "template": [
        "default",
        "templates/darkfx"
      ]
  }
}