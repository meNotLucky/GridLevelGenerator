{
  "metadata": [
      {
          "src": [
              {
                  "src": "..",
                  "files": [
                    "Assets/LevelGenerator/Generator/*.cs",
                    "Assets/LevelGenerator/Utility/GridLevelUtility.cs"
                  ]
              }
          ],
          "globalNamespaceId": "Global",
          "filter": "filterConfig.yml",
          "dest": "api"
      }
  ],
  "build": {
      "globalMetadata": {
          "_appTitle": "Grid Level Generator Documentation",
          "_appFooter": "Grid Level Generator Documentation",
          "_enableSearch": true
      },
      "content": [
          {
              "files": [
                  "toc.yml",
                  "index.md"
              ]
          },
          {
              "src": "api",
              "files": [
                  "*.yml"
              ],
              "dest": "api"
          },
          {
              "src": "manual",
              "files": [
                  "toc.yml",
                  "*.md"
              ],
              "dest": "manual"
          }
      ],
      "overwrite": [
          {
              "src": "..",
              "files": [
                  "Assets/GridLevelGenerator/**/*.md"
              ]
          }
      ],
      "resource": [
          {
              "files": [
                  "resources/**/*"
              ]
          }
      ],
      "sitemap": {
          "baseUrl": "https://menotlucky.github.io/GridLevelGenerator",
          "changefreq": "weekly",
          "fileOptions": {
              "api/*": {
                  "changefreq": "daily"
              }
          }
      },
      "xref": [
          "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
      ],
      "xrefService": [
          "https://xref.docs.microsoft.com/query?uid={uid}"
      ],
      "dest": "../_site"
  }
}